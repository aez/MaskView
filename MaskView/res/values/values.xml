<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaskView">
        <!-- Layout ID of the view that will show through the "holes" in the
        MaskView. -->
        <attr name="backgroundView" format="reference"/>
        <!-- Resource ID of the alpha mask that defines the "holes" in the
        MaskView.  This can be a 9-patch. -->
        <attr name="alphaMask" format="reference"/>
        <!-- Resource ID of the foreground overlay.  This can be a 9-patch. -->
        <attr name="foreground" format="reference"/>
    </declare-styleable>
</resources>
